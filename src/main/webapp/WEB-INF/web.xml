<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProducerServlet</servlet-name>
  	<display-name>ProducerServlet</display-name>
  	<description></description>
  	<servlet-class>service.ProducerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loadchatservice</servlet-name>
  	<display-name>loadchatservice</display-name>
  	<description></description>
  	<servlet-class>service.LoadChatService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PublishService</servlet-name>
  	<display-name>PublishService</display-name>
  	<description></description>
  	<servlet-class>service.PublishService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProducerServlet</servlet-name>
  	<url-pattern>/producerservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loadchatservice</servlet-name>
  	<url-pattern>/loadchatservice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PublishService</servlet-name>
  	<url-pattern>/publishservice</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>service.AppContextListener</listener-class>
</listener>
</web-app>
